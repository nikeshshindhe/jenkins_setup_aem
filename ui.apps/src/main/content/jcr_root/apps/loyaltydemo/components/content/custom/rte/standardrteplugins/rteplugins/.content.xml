<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <format
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <justify
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <lists
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <paraformat
        jcr:primaryType="nt:unstructured"
        features="*">
        <formats jcr:primaryType="cq:WidgetCollection">
            <paragraph
                jcr:primaryType="nt:unstructured"
                description="Paragraph"
                tag="p"/>
            <h1
                jcr:primaryType="cq:WidgetCollection"
                description="H1"
                tag="h1"/>
            <h2
                jcr:primaryType="nt:unstructured"
                description="H2"
                tag="h2"/>
            <h3
                jcr:primaryType="nt:unstructured"
                description="H3"
                tag="h3"/>
            <h4
                jcr:primaryType="nt:unstructured"
                description="H4"
                tag="h4"/>
            <h5
                jcr:primaryType="nt:unstructured"
                description="H5"
                tag="h5"/>
            <h6
                jcr:primaryType="nt:unstructured"
                description="H6"
                tag="h6"/>
            <small
                jcr:primaryType="nt:unstructured"
                description="Small"
                tag="small"/>
            <br
                jcr:primaryType="nt:unstructured"
                description="br"
                tag="br"/>
            <div
                jcr:primaryType="nt:unstructured"
                description="div"
                tag="div"/>
        </formats>
    </paraformat>
    <links
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <image
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <findreplace
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <subsuperscript
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <spellcheck
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <misctools
        jcr:primaryType="nt:unstructured"
        features="*">
        <specialCharsConfig jcr:primaryType="nt:unstructured">
            <chars jcr:primaryType="nt:unstructured">
                <copyright
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#169;"/>
                <trademark
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#8482;"/>
                <registered
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#174;"/>
                <emDash
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#8212;"/>
                <pound
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#163;"/>
                <nbsp
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#160;"/>
            </chars>
        </specialCharsConfig>
    </misctools>
    <styles
        jcr:primaryType="nt:unstructured"
        features="*">
        <styles jcr:primaryType="nt:unstructured">
        	<username
                jcr:primaryType="nt:unstructured"
                cssName="loyaltyUsername"
                text="Username"/>
            <userFirstName
                jcr:primaryType="nt:unstructured"
                cssName="userFirstName"
                text="First Name"/>
            <userLastName
            	jcr:primaryType="nt:unstructured"
                cssName="userLastName"
                text="Last Name"/>
            <userPrimaryEmail
            	jcr:primaryType="nt:unstructured"
                cssName="userPrimaryEmail"
                text="Email Address"/>
            <userPrimaryPhone
            	jcr:primaryType="nt:unstructured"
                cssName="userPrimaryPhone"
                text="Phone Number"/>
            <userTier
                jcr:primaryType="nt:unstructured"
                cssName="userTier"
                text="Current Tier"/>
            <userPointsTypeDesc
                jcr:primaryType="nt:unstructured"
                cssName="userPointsTypeDesc"
                text="Points Type Description"/>
            <userPooledPointsIndicator
                jcr:primaryType="nt:unstructured"
                cssName="userPooledPointsIndicator"
                text="Display Pooled Points Indicator"/>
            <userPoints
                jcr:primaryType="nt:unstructured"
                cssName="userPoints"
                text="Available Points"/>
            <userNextTier
            	jcr:primaryType="nt:unstructured"
                cssName="userNextTier"
                text="Next Tier"/>
            <userPointsToNextTier
            	jcr:primaryType="nt:unstructured"
                cssName="userPointsToNextTier"
                text="Points To Next Tier"/>
            <userCardNumber
                jcr:primaryType="nt:unstructured"
                cssName="userCardNumber"
                text="Loyalty Card Number"/>
            <loyaltyGamerAlias
                jcr:primaryType="nt:unstructured"
                cssName="loyaltyGamerAlias"
                text="Loyalty Gamer Alias"/>
            <userDateOfBirth
                jcr:primaryType="nt:unstructured"
                cssName="userDateOfBirth"
                text="Date of Birth"/>
            <userJoinDate
                jcr:primaryType="nt:unstructured"
                cssName="userJoinDate"
                text="Join Date"/>
        </styles>
    </styles>
    <table
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <experience-aem
        jcr:primaryType="nt:unstructured"
        features="*"/>
</jcr:root>
