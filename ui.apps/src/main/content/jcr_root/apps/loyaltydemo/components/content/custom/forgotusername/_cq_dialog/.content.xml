<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Forgot Username"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <forgotusernametab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Forgot Username"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Heading"
                                        name="./heading"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/rteplugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/uisettings"/>
                                    </heading>
                                    <emailaddresslabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Email Address Label"
                                        name="./emailAddressLabel"
                                        required="{Boolean}true"/>
                                    <forgotusernamecta
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ctafieldset
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Forgot Username CTA"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ctacontainer
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container"
                                                        name="./forgotUsernameCta">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ctalabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Label"
                                                                name="./forgotUsernameCta/ctaLabel"/>
                                                            <ctatype
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                emptyText="CTA Type"
                                                                fieldLabel="CTA Type"
                                                                name="./forgotUsernameCta/ctaType">
                                                                <datasource
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                                    path="/etc/acs-commons/lists/loyaltydemo/cta-type-options"/>
                                                            </ctatype>
                                                        </items>
                                                    </ctacontainer>
                                                </items>
                                            </ctafieldset>
                                        </items>
                                    </forgotusernamecta>
                                </items>
                            </column>
                        </items>
                    </forgotusernametab>
                    <completiontab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Completion"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Heading"
                                        name="./completionHeading"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/rteplugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/uisettings"/>
                                    </heading>
                                    <completioncta
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ctafieldset
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Completion CTA"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ctacontainer
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container"
                                                        name="./completionCta">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ctalabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Label"
                                                                name="./completionCta/ctaLabel"/>
                                                            <ctatype
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                emptyText="CTA Type"
                                                                fieldLabel="CTA Type"
                                                                name="./completionCta/ctaType">
                                                                <datasource
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                                    path="/etc/acs-commons/lists/loyaltydemo/cta-type-options"/>
                                                            </ctatype>
                                                        </items>
                                                    </ctacontainer>
                                                </items>
                                            </ctafieldset>
                                        </items>
                                    </completioncta>
                                </items>
                            </column>
                        </items>
                    </completiontab>
                    <errormessagetab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Message"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <emailmissing
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Email Missing Message"
                                        name="./emailMissing"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/rteplugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/uisettings"/>
                                    </emailmissing>
                                    <emailformat
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Invalid Email Message"
                                        name="./emailFormat"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/rteplugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="loyaltydemo/components/content/custom/rte/standardrteplugins/uisettings"/>
                                    </emailformat>
                                </items>
                            </column>
                        </items>
                    </errormessagetab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
